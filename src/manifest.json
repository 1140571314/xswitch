{
  "name": "XSwitch",
  "description": "XSwitch tools for proxy web request url, support reg",
  "short_name": "xs",
  "version": "1.1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "XSwitch",
    "default_popup": "XSwitch.html"
  },
  "permissions": ["webRequest", "storage", "webRequestBlocking", "<all_urls>"],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
