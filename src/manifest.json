{
  "name": "XSwitch",
  "description": "XSwitch tools for proxy web request url, support reg",
  "short_name": "xs",
  "version": "1.3.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/blue_128.png",
    "default_title": "XSwitch",
    "default_popup": "XSwitch.html"
  },
  "permissions": ["webRequest", "storage", "webRequestBlocking", "browsingData", "<all_urls>"],
  "icons": {
    "48": "images/blue_128.png",
    "128": "images/blue_128.png"
  },
  "background": {
    "scripts": ["forward.js", "background.js"]
  }
}
